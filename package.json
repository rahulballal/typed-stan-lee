  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build/",
    "compile": "tsc",
    "db:seed": "ts-node-dev src/util/seed-db.ts",
    "dev": "NODE_ENV=development tsnd --respawn --transpileOnly ./src/index.ts",
    "lint:fix": "tslint -c ./tslint.json './src/**/*.ts' --fix",
    "lint": "tslint -c ./tslint.json './src/**/*.ts'",
    "prod": "NODE_ENV=production node .",
    "start": "run-s clean compile prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.4.0",
    "graphql": "^14.1.1",
    "pg": "^7.8.0",
    "ramda": "^0.26.1",
    "reflect-metadata": "^0.1.13",
    "shortid": "^2.2.14",
    "type-graphql": "^0.16.0",
    "typeorm": "0.2.12"
  },
  "devDependencies": {
    "@types/micro": "^7.3.3",
    "@types/node": "^10.12.24",
    "@types/ramda": "^0.25.48",
    "@types/shortid": "^0.0.29",
    "jest": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3"
  },
  "jest": {
    "globalSetup": "./src/testUtils/callSetup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
