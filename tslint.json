{
  "linterOptions": {
    "exclude": [
      "node_modules",
      "dist"
    ]
  },
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name": false,
    "no-submodule-imports": true
  },
  "rulesDirectory": []
}